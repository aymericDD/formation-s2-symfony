{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Ip</h1>

    <table class="record_properties table-bordered table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ entity.ip }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
        </tbody>
    </table>


    <ul class="record_actions list-unstyled">
        <li>
            <a class="btn-danger btn" href="{{ path('ip') }}">
                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> Back to the list
            </a>
            <a class="btn-success btn" href="{{ path('ip_edit', { 'id': entity.id }) }}">
                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock %}
